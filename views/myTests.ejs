<div>
    <h1>Here are the other tests</h1>
    <% if (tests.length > 0) { %>
        <div>
            <% tests.forEach(test => { %>
                <div class="container">
                    <h2><%= test.name %></h2>
                    <p><%= test.description %></p>
                    <a href="/test/<%= test._id %>">Take this test</a>
                </div>
            <% }); %>
        </div>
    <% } else { %>
        <p>No tests found.</p>
    <% } %>
  </div>