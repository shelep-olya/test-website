<div>
    <h1>Here are the other tests</h1>
    <% if (data.tests.length > 0) { %>
      <div>
        <% data.tests.forEach(test => { %>
          <div class="container">
            <h2><%= test.name %></h2>
            <p><%= test.author %></p>
            <p><%= test.description %></p>
          </div>
        <% }) %>
      </div>
    <% } else { %>
      <p>No tests available</p>
    <% } %> 
  </div>